                       FontWeight="SemiBold"
                       FontFamily="Courier New"
                       FontSize="18"
                       Margin="3"
                       HorizontalAlignment="Center" />
            <TextBlock Grid.Column="6" Foreground="#FF27363F"
                       FontWeight="SemiBold"
                       FontFamily="Courier New"
                       FontSize="18"
                       Margin="3"
                       HorizontalAlignment="Center" />
            <TextBlock Grid.Column="7" Foreground="#FF27363F"
                       FontWeight="SemiBold"
                       FontFamily="Courier New"
                       FontSize="18"
                       Margin="3"
                       HorizontalAlignment="Center" />

            <Grid Background="Gray" Grid.Row="1" Margin="0,0,1,0">
